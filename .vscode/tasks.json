{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-electron",
      "type": "shell",
      "command": "npm run build:electron:main",
      "problemMatcher": []
    },
    {
      "label": "start-vite",
      "type": "shell",
      "command": "npm run dev",
      "isBackground": true,
      "problemMatcher": {
        "pattern": {
          "regexp": "^(.*)$",
          "file": 1,
          "location": 2,
          "message": 3
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": ".*ready in.*"
        }
      }
    },
    {
      "label": "prepare-debug-complete",
      "dependsOn": ["build-electron", "start-vite"],
      "dependsOrder": "sequence",
      "problemMatcher": []
    }
  ]
}
